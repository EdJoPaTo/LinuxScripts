{
	"deno.cacheOnSave": true,
	"diffEditor.experimental.showMoves": true,
	"diffEditor.wordWrap": "on",
	"editor.accessibilitySupport": "off",
	"editor.stickyScroll.enabled": true,
	"editor.wordWrap": "on",
	"editor.wrappingIndent": "indent",
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	"files.autoSave": "onFocusChange",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"git.openRepositoryInParentFolders": "never",
	"git.suggestSmartCommit": false,
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"ltex.additionalRules.motherTongue": "de-DE",
	"platformio-ide.disablePIOHomeStartup": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": false,
	"rust-analyzer.assist.emitMustUse": true,
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.completion.privateEditable.enable": true,
	"rust-analyzer.imports.granularity.enforce": true,
	"rust-analyzer.imports.granularity.group": "module",
	"rust-analyzer.imports.merge.glob": false,
	"rust-analyzer.inlayHints.bindingModeHints.enable": true,
	"rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
	"rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
	"rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
	"rust-analyzer.inlayHints.rangeExclusiveHints.enable": true,
	"rust-analyzer.references.excludeImports": true,
	"rust-analyzer.showUnlinkedFileNotification": false,
	"rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
	"scm.defaultViewMode": "tree",
	"terminal.external.linuxExec": "alacritty",
	"terminal.integrated.stickyScroll.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.autoDetectColorScheme": true,
	"window.commandCenter": false,
	"window.menuBarVisibility": "toggle",
	"window.restoreWindows": "none",
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.perEditorGroup": true,
	"workbench.editor.limit.value": 6,
	"workbench.layoutControl.enabled": false,
	"workbench.startupEditor": "none",
	"workbench.tree.enableStickyScroll": true,
	"[json]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno",
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		}
	},
	"editor.rulers": [
		80,
		100,
		160
	],
	"markdownlint.config": {
		"MD010": false,
		"MD013": false,
		"MD024": {
			"siblings_only": true
		}
	},
	"rust-analyzer.check.extraArgs": [
		"--all-features",
		"--keep-going",
		"--no-deps",
		"--",
		"-Wclippy::min_ident_chars",
		"-Aclippy::cargo"
	],
	"diffEditor.ignoreTrimWhitespace": false
}
