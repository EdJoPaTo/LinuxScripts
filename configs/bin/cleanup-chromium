#!/usr/bin/env sh
set -ux

killall -9 chromium
rm -rf ~/.cache/chromium
rm -rf ~/.config/*chromium*
rm -rf ~/Library/Application\ Support/Chromium
rm -rf ~/Library/Application\ Support/Google
rm -rf ~/Library/Caches/Chromium
rm -rf ~/Library/Caches/Google
rm -rf ~/Library/Caches/com.google.*
rm -rf ~/Library/Google
rm -rf ~/Library/HTTPStorages/com.google.*
rm -rf ~/Library/LaunchAgents/com.google.*
rm -rf ~/Library/Preferences/*Chrom*
rm -rf ~/Library/Preferences/*chromium*
rm -rf ~/Library/Preferences/com.google.*
rm -rf ~/Library/Saved\ Application\ State/*Chrom*
rm -rf ~/Library/Saved\ Application\ State/*chromium*
